clear lock
clear shift
clear mod1
clear mod2
clear mod3
clear mod4
clear mod5

keycode 9  = Escape Caps_Lock  NoSymbol NoSymbol NoSymbol
keycode 23 = Escape NoSymbol Escape NoSymbol Escape
keycode 37 = Tab ISO_Left_Tab Tab ISO_Left_Tab Tab ISO_Left_Tab

add lock = Caps_Lock
add control = Control_L
add shift = Shift_L Shift_R
add mod1 = Alt_L
add mod2 = Num_Lock
add mod3 = Hyper_L
add mod4 = Super_L Super_R
add mod5 = Mode_switch ISO_Level3_Shift


